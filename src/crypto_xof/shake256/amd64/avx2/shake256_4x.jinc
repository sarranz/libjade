
from Jade require "common/keccak/keccak1600/amd64/avx2/keccakf1600_4x.jinc"
from Jade require "common/keccak/common/fips202_params.jinc" // SHAKE256_RATE

inline fn __shake256_squeezeblock4x(
  #[public, poly=tstate] reg ptr u256[25] state,
  #[public, poly=t0] reg ptr u8[SHAKE256_RATE] h0,
  #[public, poly=t1] reg ptr u8[SHAKE256_RATE] h1,
  #[public, poly=t2] reg ptr u8[SHAKE256_RATE] h2,
  #[public, poly=t3] reg ptr u8[SHAKE256_RATE] h3,
  #msf reg u64 ms
) ->
  #[public, poly=tstate] reg ptr u256[25],
  #[public, poly=t0] reg ptr u8[SHAKE256_RATE],
  #[public, poly=t1] reg ptr u8[SHAKE256_RATE],
  #[public, poly=t2] reg ptr u8[SHAKE256_RATE],
  #[public, poly=t3] reg ptr u8[SHAKE256_RATE],
  #msf reg u64
{
  reg u256 t256;
  reg u128 t128;
  inline int i;

  state = __keccakf1600_4x_avx2(state);

	for i = 0 to (SHAKE256_RATE / 8) {
    t256 = state[i];
    t128 = (128u)t256;
		h0[u64 i] = #VMOVLPD(t128);
		h1[u64 i] = #VMOVHPD(t128);
    t128 = #VEXTRACTI128(t256, 1);
		h2[u64 i] = #VMOVLPD(t128);
		h3[u64 i] = #VMOVHPD(t128);
	}

  ms = #init_msf();
  return state, h0, h1, h2, h3, ms;
}
